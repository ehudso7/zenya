{
  "name": "zenya-ai",
  "version": "1.0.0",
  "description": "AI-powered learning platform for neurodiverse adults",
  "private": true,
  "homepage": "https://zenyaai.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehudso7/zenya.git"
  },
  "bugs": {
    "url": "https://github.com/ehudso7/zenya/issues"
  },
  "author": {
    "name": "Zenya AI",
    "email": "support@zenyaai.com",
    "url": "https://zenyaai.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:ws": "node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:collaborative": "tsx run-collaborative-tests.ts",
    "test:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "test:load": "k6 run k6/load-test.js",
    "test:stress": "k6 run k6/stress-test.js",
    "security:scan": "snyk test",
    "security:monitor": "snyk monitor",
    "lighthouse": "node scripts/lighthouse-performance-check.js",
    "performance:check": "npm run lighthouse",
    "performance:ci": "lhci autorun",
    "security:audit": "node scripts/security-audit.js",
    "security:full": "npm run security:audit && npm run security:scan",
    "prepare-deploy": "npm run test && npm run build",
    "format": "prettier --write .",
    "tf:init": "cd terraform && terraform init",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply"
  },
  "keywords": [
    "ai",
    "learning",
    "education",
    "adhd",
    "neurodiversity",
    "tutoring",
    "adaptive-learning",
    "pwa",
    "nextjs"
  ],
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.27.3"
  },
