    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/ws": "^8.5.12",
    "autoprefixer": "^10.4.21",
    "chrome-launcher": "^1.1.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "lighthouse": "^12.0.0",
    "postcss": "^8.5.4",
    "prettier": "^3.1.0",
