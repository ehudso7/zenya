{
  "name": "zenya-ai",
  "version": "1.0.0",
  "description": "AI-powered learning platform for neurodiverse adults",
  "private": true,
  "homepage": "https://zenyaai.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehudso7/zenya.git"
  },
  "bugs": {
    "url": "https://github.com/ehudso7/zenya/issues"
  },
  "author": {
    "name": "Zenya AI",
    "email": "support@zenyaai.com",
    "url": "https://zenyaai.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/verify-domain.js && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "test:load": "k6 run k6/load-test.js",
    "test:stress": "k6 run k6/stress-test.js",
    "security:scan": "snyk test",
    "security:monitor": "snyk monitor",
    "prepare-deploy": "npm run test && npm run build",
    "format": "prettier --write .",
    "tf:init": "cd terraform && terraform init",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply"
  },
  "keywords": [
    "ai",
    "learning",
    "education",
    "adhd",
    "neurodiversity",
    "tutoring",
    "adaptive-learning",
    "pwa",
    "nextjs"
  ],
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.24.3"
  },
  "dependencies": {
    "@datadog/browser-logs": "^5.0.0",
    "@datadog/browser-rum": "^5.0.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@sentry/nextjs": "^9.24.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@types/nodemailer": "^6.4.17",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "dd-trace": "^5.0.0",
    "framer-motion": "^12.15.0",
    "hot-shots": "^10.0.0",
    "lucide-react": "^0.511.0",
    "next": "^15.3.3",
    "openai": "^5.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "resend": "^4.5.2",
    "tailwind-merge": "^3.3.0",
    "unleash-proxy-client": "^3.3.0",
    "zod": "^3.25.51",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@supabase/supabase-js": "^2.49.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "postcss": "^8.5.4",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  }
